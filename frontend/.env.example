# Frontend example env for MailFortress (Vite)
# Copy to .env.local and replace values. Do NOT commit secrets.

# Public Supabase URL (from Supabase dashboard)
VITE_SUPABASE_URL=https://<your-project-ref>.supabase.co

# Public (anon) key for Supabase - safe to use in browser but keep in .env.local
VITE_SUPABASE_ANON_KEY=eyJ...your_anon_key_here

# Backend API URL (use Render URL in production)
VITE_BACKEND_URL=http://localhost:3001
